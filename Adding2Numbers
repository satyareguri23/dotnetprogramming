using System;
class program
{
public static void AddNum(int x , int y)
{
return(x+y);
}
public static void SubNum(int x , int y)
{
return(x-y);
}
static void Main()
{
AddNum(10,20);
SubNum(30,20);
Console.WriteLine("Sum of two numbers is: "+(x+y));
Console.WriteLine("Sum of two numbers is: "+(x-y));
Console.ReadLine();
}
}
