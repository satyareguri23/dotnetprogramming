using System;
class Program
{
public int x;int y;int r;int k;
public int AddNum(int x , int y)
{
r = x+y;
return(r);
}
public int SubNum(int x , int y)
{
k = x+y;
return(k);
}
static void Main()
{
Program p = new Program();
p.AddNum(10,20);
Console.WriteLine("Sum of two numbers is: "+(p.r));
p.SubNum(30,20);
Console.WriteLine("difference of two numbers is: "+(p.k));
Console.ReadLine();
}
}
